{
  "projectType": "library",
  "root": "libs/shared/util/app-config",
  "sourceRoot": "libs/shared/util/app-config/src",
  "prefix": "ahlcg-tools",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/util/app-config"],
      "options": {
        "jestConfig": "libs/shared/util/app-config/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared/util/app-config/src/**/*.ts",
          "libs/shared/util/app-config/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:shared", "type:"]
}
